{
  "name": "leet-push",
  "version": "0.0.2",
  "description": "A code repository pusher",
  "scripts": {
    "chrome-build": "cross-env TARGET=chrome gulp",
    "firefox-build": "cross-env TARGET=firefox gulp",
    "build": "cross-env NODE_ENV=production npm run chrome-build && cross-env NODE_ENV=production npm run firefox-build",
    "chrome-watch": "cross-env TARGET=chrome gulp watch",
    "firefox-watch": "cross-env TARGET=firefox gulp watch",
    "chrome-dist": "cross-env NODE_ENV=production cross-env TARGET=chrome gulp dist",
    "firefox-dist": "cross-env NODE_ENV=production cross-env TARGET=firefox gulp dist",
    "dist": "npm run chrome-dist && npm run firefox-dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vinhnguyen500/leetpush"
  },
  "author": "Vinh <vinhnguyen500@hotmail.com> (https://github.com/vinhnguyen500)",
  "bugs": {
    "url": "https://github.com/vinhnguyen500/leetpush/issues"
  },
  "homepage": "https://www.emailthis.me/extension-boilerplate",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "cross-env": "^3.2.4",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.0",
    "gulp-clean": "^0.3.1",
    "gulp-eslint": "^2.0.0",
    "gulp-if": "^2.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^0.5.3",
    "gulp-merge-json": "^1.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-run-sequence": "*",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
